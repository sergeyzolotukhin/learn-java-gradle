<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%-10.10thread] %-5level | %-20.20X{propertyPath} | %-60.60X{object} | %-40.40logger{36} - %msg %n</pattern>
        </encoder>
    </appender>

    <logger name="ua.in.sz.predicate.Application" level="trace" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>