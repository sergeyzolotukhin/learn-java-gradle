<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
">
    <context:property-placeholder location="classpath:application.properties,second-application.properties" />

    <bean id="businessService" class="ua.in.sz.h2.BusinessService">
        <property name="myName" value="${my.name}"/>
        <property name="mySecondName" value="${my.second.name}"/>
    </bean>
</beans>