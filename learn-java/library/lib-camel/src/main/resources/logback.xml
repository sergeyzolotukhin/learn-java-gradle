<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%-50.50thread] %-5level | %-40.40logger{36} - %msg %n</pattern>
        </encoder>
    </appender>

<!--    <logger name="ua.in.sz.camel.AtLeastOneSuccessAggregationStrategy" level="trace" />-->

    <logger name="org.apache.camel" level="warn" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>