<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%white(%d{HH:mm:ss.SSS}) [%blue(%-10.10thread)] %highlight(%-5level) | %yellow(%-40.40logger{36}) - %msg %n</pattern>
        </encoder>
    </appender>

    <logger name="ua.in.sz.h2.ExecutionTimeResolverFactory" level="debug" />

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>