<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} %-5level | %-25.25logger{36} - %msg %n</pattern>
        </encoder>
    </appender>

    <logger name="ua.in.sz.house.house.Wall" level="debug"/>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>